{
	"name": "quickcd",
	"displayName": "quickcd",
	"description": "Free you of cd a file.",
	"publisher": "alightwang",
	"version": "0.0.13",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:quickcd.open",
		"onCommand:quickcd.default_terminal",
		"onCommand:quickcd.terminal",
		"onCommand:quickcd.cd",
		"onCommand:quickcd.openInVS",
		"onCommand:quickcd.fileExplorer"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"menus": {
			"editor/context": [
				{
					"command": "quickcd.cd",
					"group": "quickcd@2",
					"when": "editorTextFocus&&resourceLangId == markdown"
				},
				{
					"command": "quickcd.open",
					"group": "quickcd@3",
					"when": "editorTextFocus&&resourceLangId == markdown"
				},
				{
					"command": "quickcd.openInVS",
					"group": "quickcd@4",
					"when": "editorTextFocus&&resourceLangId == markdown"
				},
				{
					"command": "quickcd.fileExplorer",
					"group": "quickcd@5",
					"when": "editorTextFocus&&resourceLangId == markdown"
				},
				{
					"command": "quickcd.default_terminal",
					"group": "quickcd@6",
					"when": "editorTextFocus&&resourceLangId == markdown"
				},
				{
					"command": "quickcd.terminal",
					"group": "quickcd@7",
					"when": "editorTextFocus&&resourceLangId == markdown"
				}
			]
		},
		"commands": [
			{
				"command": "quickcd.default_terminal",
				"title": "quickcd: default terminal"
			},
			{
				"command": "quickcd.terminal",
				"title": "quickcd: create terminal"
			},
			{
				"command": "quickcd.terminal_3",
				"title": "quickcd: create terminal3"
			},
			{
				"command": "quickcd.open",
				"title": "quickcd: open"
			},
			{
				"command": "quickcd.cd",
				"title": "quickcd: cd"
			},
			{
				"command": "quickcd.openInVS",
				"title": "quickcd: open in VS"
			},
			{
				"command": "quickcd.fileExplorer",
				"title": "quickcd: file explorer"
			}
		],
		"configuration": {
			"title": "quickcd",
			"type": "object",
			"properties": {
				"quickcd": {
					"type": "object",
					"properties": {
						"defaultPath": {
							"type": "string",
							"default": "C:\\Substrate\\src"
						},
						"paths": {
							"type": "string",
							"default": ""
						}
					}
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}